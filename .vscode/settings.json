def rechercher_medicament(nom):
    for med in medicaments:
        if med["nom"].lower() == nom.lower():
            return med
    return None


nom_recherche = input("Entrer le nom du médicament : ")
resultat = rechercher_medicament(nom_recherche)

if resultat:
    print("Médicament trouvé ✅")
    print(resultat)
else:
    print("Médicament non trouvé ❌")